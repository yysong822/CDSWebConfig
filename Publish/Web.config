    <add key="PeriodTime_VerifyExpression" value="^(0|[1-9][0-9]*)$" />
    <add key="FileDir_Label" value="监控文件目录：" />
    <add key="FileDir_Tip" value="要监控的文件目录" />
    <add key="File_Label" value="监控文件名称：" />
